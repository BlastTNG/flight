install_log: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/data/etc/cow/ || $(MKDIR) $(INSTALL_ROOT)/data/etc/cow/ 
	-$(INSTALL_FILE) /home/peter/git/flight/cow/data/prev_status $(INSTALL_ROOT)/data/etc/cow/
	-$(INSTALL_FILE) /home/peter/git/flight/cow/data/log.txt $(INSTALL_ROOT)/data/etc/cow/


uninstall_log:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/data/etc/cow/prev_status 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/data/etc/cow/log.txt
	-$(DEL_DIR) $(INSTALL_ROOT)/data/etc/cow/ 


