install_log: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/data/etc/narsil/ || $(MKDIR) $(INSTALL_ROOT)/data/etc/narsil/ 
	-$(INSTALL_FILE) /data1/home/joshua/narsil4/data/prev_status $(INSTALL_ROOT)/data/etc/narsil/
	-$(INSTALL_FILE) /data1/home/joshua/narsil4/data/log.txt $(INSTALL_ROOT)/data/etc/narsil/


uninstall_log:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/data/etc/narsil/prev_status 
