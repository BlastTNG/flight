#! /bin/sh

echo // This file is autogenerated by doGetFieldList.  Any manual changes you make > framelist.inc
echo // will be lost the next time someone runs it. >> framelist.inc

cat tx_struct.c | awk '($1~"{")&&($1!~"/"){print}' | awk -F \, '(($1!~"status0") && ($1!~"status1") && ($1!~"sync1") && ($1!~"sync0") && ($1!~"crc_ok") && ($1!~"chatter") && ($1!~"polarity") && ($1!~"decom_unlock")) {printf "%s,\n", $1}' | sed 's/{//' >> framelist.inc
