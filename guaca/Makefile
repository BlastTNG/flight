####### Compile

main.o: main.cpp mainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

mainwindow.o: mainwindow.cpp ../external_libs/linklist/linklist.h \
		../external_libs/linklist/linklist_connect.h \
		mainwindow.h \
		ui_mainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mainwindow.o mainwindow.cpp

linklist.o: ../external_libs/linklist/linklist.c ../external_libs/linklist/linklist.h \
		../external_libs/linklist/linklist_compress.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o linklist.o ../external_libs/linklist/linklist.c

linklist_compress.o: ../external_libs/linklist/linklist_compress.c ../external_libs/linklist/linklist.h \
		../external_libs/linklist/linklist_compress.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o linklist_compress.o ../external_libs/linklist/linklist_compress.c

linklist_writer.o: ../external_libs/linklist/linklist_writer.c ../external_libs/linklist/linklist.h \
		../external_libs/linklist/linklist_compress.h \
		../external_libs/linklist/linklist_writer.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o linklist_writer.o ../external_libs/linklist/linklist_writer.c

linklist_connect.o: ../external_libs/linklist/linklist_connect.c ../external_libs/linklist/linklist.h \
		../external_libs/linklist/linklist_writer.h \
		../external_libs/linklist/linklist_connect.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o linklist_connect.o ../external_libs/linklist/linklist_connect.c

CRC_func.o: ../common/CRC_func.c ../common/include/CRC_func.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o CRC_func.o ../common/CRC_func.c

qrc_guacapics.o: qrc_guacapics.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o qrc_guacapics.o qrc_guacapics.cpp

moc_mainwindow.o: moc_mainwindow.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_mainwindow.o moc_mainwindow.cpp

####### Install

install_desktopfile: first FORCE
	@test -d $(INSTALL_ROOT)/usr/share/applications || mkdir -p $(INSTALL_ROOT)/usr/share/applications
	-$(INSTALL_FILE) /home/javier/git/BLAST_TNG/guaca/guaca.desktop $(INSTALL_ROOT)/usr/share/applications/

uninstall_desktopfile: FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/applications/guaca.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/applications/ 


install_iconfile: first FORCE
	@test -d $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps || mkdir -p $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps
	-$(INSTALL_FILE) /home/javier/git/BLAST_TNG/guaca/images/guaca.svg $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/

uninstall_iconfile: FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/guaca.svg
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/icons/hicolor/scalable/apps/ 


install_target: first FORCE
	@test -d $(INSTALL_ROOT)/usr/local/bin || mkdir -p $(INSTALL_ROOT)/usr/local/bin
	-$(INSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)
	-$(STRIP) $(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install: install_desktopfile install_iconfile install_target  FORCE

uninstall: uninstall_desktopfile uninstall_iconfile uninstall_target  FORCE

FORCE:

